                    <module>Siliconrockstar_Adminsinglesession</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </siliconrockstar_adminsinglesession_setup>
            <siliconrockstar_adminsinglesession_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </siliconrockstar_adminsinglesession_write>
            <siliconrockstar_adminsinglesession_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </siliconrockstar_adminsinglesession_read>
        </resources>
        <events>
